diff --git a/url2GSI.sh b/url2GSI.sh
index 89abcde..76cdeff 100755
--- a/url2GSI.sh
+++ b/url2GSI.sh
@@
 OTA_URL="$1"

+# Extract OTA filename
+FILENAME=$(basename "$OTA_URL")
+
+# Check if OTA is already downloaded
+if [ -f "$FILENAME" ]; then
+    echo "‚úîÔ∏è OTA already downloaded: $FILENAME ‚Äî skipping"
+else
+    echo "‚è¨ Downloading OTA: $FILENAME"
+    wget -c "$OTA_URL" -O "$FILENAME"
+    if [ $? -ne 0 ]; then
+        echo "‚ùå Download failed for $OTA_URL"
+        exit 1
+    fi
+fi
+
+# Extract payload.bin if present
+echo "üì¶ Extracting OTA ZIP..."
+unzip -o "$FILENAME"
+
+if [ -f payload.bin ]; then
+    echo "üîç Found payload.bin ‚Äî dumping images..."
+    mkdir -p extracted
+    ./bin/payload-dumper-go -o extracted payload.bin || python3 payload_dumper.py payload.bin
+    cp extracted/*.img .
+else
+    echo "‚ö†Ô∏è payload.bin not found ‚Äî skipping payload extraction"
+fi
+
+# Detect init_boot.img
+if [ -f init_boot.img ]; then
+    echo "üß© Extracting init_boot.img"
+    mkdir -p ramdisk/init_boot
+    ./bin/magiskboot unpack init_boot.img ramdisk/init_boot || echo "‚ö†Ô∏è init_boot.img extraction failed"
+fi
+
+case "$VERSION" in
+    10|11|12|13|14)
+        echo "‚úîÔ∏è Recognized Android $VERSION"
+        ;;
+    16)
+        echo "‚ö†Ô∏è Android 16 (Vanilla Ice Cream) detected ‚Äî full image extraction enabled"
+        ;;
+    *)
+        echo "‚ùå Unsupported Android version: $VERSION"
+        exit 1
+        ;;
+esac
